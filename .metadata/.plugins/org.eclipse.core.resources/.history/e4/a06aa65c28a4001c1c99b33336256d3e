
public class Ex05 {

	public static void main(String[] args) {
		
		int seats[][] = new int[11][11];
		
		for(int i = 0; i < seats.length; i++) {
			for(int j = 0; j < seats[i].length; j++) {
				System.out.println("["+i+"]");
				System.out.print("[□]");
			}
			System.out.print("[행]");
			System.out.println();
		}

	}

}
