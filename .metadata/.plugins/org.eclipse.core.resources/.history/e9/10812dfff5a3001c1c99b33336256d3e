
public class Practice02Variable {

	public static void main(String[] args) {
		// 2021/10/06_java_실습
		
		/*				1byte			2byte			4byte			8byte
		 * 정수/문자		byte			short/char		int(Integer)	long
		 * 실수											float			double
		 * 논리			boolean
		 */
		
		String str = "Hello Java!!!";
		System.out.println(str);

	}

}
