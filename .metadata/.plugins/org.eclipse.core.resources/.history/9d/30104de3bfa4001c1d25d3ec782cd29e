package final_test;

public class FinalTest {

	int a = 10;
	final int b = 10;
	final int c;
	
	static final double PI = 3.14;
	final static int MAX_NUMBER = 25;
	
	FinalTest(){
		c = 100;
	}
	
	public static void main(String[] args) {
		FinalTest finalTest = new FinalTest();
		System.out.println(finalTest);
		finalTest.a = 30;

	}

}
