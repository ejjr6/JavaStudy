package b.string.string_builder;

public class StringBuilderExam {

	public static void main(String[] args) {
		String a = "최기근";
		System.out.println(a.hashCode());
		a += "천재";
		System.out.println(a.hashCode());
		a += "다";
		System.out.println(a.hashCode());
		
		StringBuilder sb = new StringBuilder("초기값 : ");
		
	}

}
