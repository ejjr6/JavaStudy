
public class Ex05 {

	public static void main(String[] args) {
		
		int seats[][] = new int[10][10];
		
		for(int i = 0; i < seats.length; i++) {
			System.out.print("["+(i+1)+"]");
		}
		System.out.println("[행]");
		
		for(int i = 0; i< seats[i].length; i++) {
			System.out.print("[□]");
		}
		System.out.print("[A열]");
	}

}
