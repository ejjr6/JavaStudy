package a.object.clone;

import java.util.Arrays;

public class Member implements Cloneable
{
	public String name;
	public int age;
	public int[] score;
	public Car car;
	
	public Member(String name, int age, int[] scroe, Car car) {
		this.name = name;
		this.age = age;
		this.scroe = scroe;
		this.car = car;
	}

	@Override
	public String toString() {
		return "Member [name=" + name + ", age=" + age + ", scroe=" + Arrays.toString(scroe) + ", car=" + car + "]";
	}
	
	public Member getMember() {
		Member cloned = null;
		
		try {
			cloned = (Member)super.clone();
		} catch (CloneNotSupportedException e) {
			e.printStackTrace();
		}
		
		/*
		try {
			cloned = (Member)clone();
		} catch (CloneNotSupportedException e) {
			e.printStackTrace();
		}
		*/
		return cloned;
	}

	@Override
	protected Object clone() throws CloneNotSupportedException {
		Member cloned = (Member)super.clone();
		int[] score = new int[this.score.length];
		return cloned;
	}
	
}
