package f.cooperation;

public class Student {
	
	String studentName;		// 학생 이름
	int grade;				// 학년
	int money;				// 학생이 가지고 있는 돈
	
	// alt + s + a
	Student(String studentName, int money) {
		this.studentName = studentName;
		this.money = money;
	}
	
	void takeBus(Bus bus) {
		bus.take(900);
		this.money -= 900;
	}
	
}
