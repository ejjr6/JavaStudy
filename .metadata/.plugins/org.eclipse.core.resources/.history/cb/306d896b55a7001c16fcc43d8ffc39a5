package c.wrapper;

public class WrapperClassExam {

	public static void main(String[] args) {
		
		Integer obj1 = new Integer(100);
		int i = obj1;
		Integer obj2 = i;
		obj2 = 100;
		obj2 = Integer.valueOf(100);
		obj2 = Integer.valueOf("100");
		System.out.println(obj2);
		
		Integer obj3 = obj1 + obj2;
		System.out.println(obj3);
	}

}
