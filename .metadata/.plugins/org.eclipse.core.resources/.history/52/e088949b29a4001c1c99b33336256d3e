
public class Ex05 {

	public static void main(String[] args) {
		
		int seats[][] = new int[10][10];
		
		for(int i = 0; i < seats.length; i++) {
			System.out.print("["+(i+1)+"]");
		}
		System.out.println("[행]");
		
		for(int i = 0; i< seats.length; i++) {
			for(int j = 0; j< seats.length; j++) {
				System.out.print("[□]");
			}
			System.out.println();
		}
	}

}
