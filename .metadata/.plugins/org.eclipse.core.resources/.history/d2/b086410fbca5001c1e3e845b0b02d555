package d.phone;

public class PhoneExample {

	public static void main(String[] args) {
		
		Phone phone = new Phone("걸리버", "silver");
		phone.powerOn();
		phone.bell();
		phone.take();
		phone.sendVocie("여보세요!!!");
		phone.receiveVoice("이번에 저희 회사에서 참좋은 보험이 나와서...");
		phone.sendVocie("죄송합니다.");
		phone.hangUp();

	}

}
