package operator;

public class OperatorArithmeticTest {

	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 20;
		// 양의 정수 표현 - 없는 것과 동일
		System.out.println(+num1);
		// 음의 정수 표현
		System.out.println(-num2);
		System.out.println(num2);
		num2 = -num2;
		System.out.println(num2);
		
		double pi = -3.141592;
		System.out.println(pi);
	}

}
