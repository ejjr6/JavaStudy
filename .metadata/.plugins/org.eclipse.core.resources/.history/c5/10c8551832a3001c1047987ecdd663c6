package operator;

public class OperatorEuqaulsTest {

	public static void main(String[] args) {
		// 비교 or 관계 연산자
		// > , >= , < , <=	좌, 우변의 값을 비교
		// 동등 비교연산자
		// == , !=
		int num1 = 10;
		int num2 = 20;
		
		boolean result = num1 > num2;
		System.out.println("num1 > num2 : " + result );

	}

}
