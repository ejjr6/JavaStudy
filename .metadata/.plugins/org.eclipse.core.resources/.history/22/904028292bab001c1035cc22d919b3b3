package mysql.a_base;

public class DataBaseExample {

	public int a;
	
	static {
		System.out.println("1 static 블럭 실행");
	}
	
	{
		a = 100;
		System.out.println("3 실행 블럭 run");
	}
	
	
	DataBaseExample() {
		System.out.println("4 a : "+a);
	}

	public static void main(String[] args) {
		System.out.println("2 Main 시작");

	}

}
